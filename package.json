{
  "name": "team-nexus-ui",
  "version": "1.0.0",
  "description": "A simple Login Page",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "babel -d ./dist ./api -s",
    "dev": "nodemon --exec babel-node ./api/index.js",
    "pretest": "npm run undo-migrate && npm run migrate",
    "test": "NODE_ENV=test nyc --reporter=lcov mocha --timeout 50000 ./__test__/test.spec.js --recursive --require @babel/register --exit || true",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "undo-migrate": "sequelize db:migrate:undo:all",
    "migrate": "sequelize db:migrate"
  },
  "nyc": {
    "exclude": [
      "config/*"
    ],
    "check-coverage": true,
    "branches": 95,
    "lines": 95,
    "functions": 95,
    "statements": 95
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cavdy/Team-Nexus-UI.git"
  },
  "author": "Team Nexus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cavdy/Team-Nexus-UI/issues"
  },
  "homepage": "https://github.com/Cavdy/Team-Nexus-UI#readme",
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.3",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "es6-promise": "^4.2.6",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "pg": "^7.8.1",
    "pg-connection-string": "^2.0.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.8.12",
    "sequelize-cli": "^5.5.0",
    "set-value": "^3.0.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.3.1",
    "codeclimate-test-reporter": "^0.5.1",
    "eslint": "^5.13.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^14.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0"
  }
}
